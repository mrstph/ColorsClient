<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ColorsClient.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ColorsClient"
    Shell.FlyoutBehavior="Flyout"
    Title="ColorsClient">

    <ShellContent
        Title="Colors Palettes"
        ContentTemplate="{DataTemplate local:ColorsPalettes}"
        Route="ColorsPalettes"/>

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:Login}"
        Route="Login"/>

    <ShellContent
        Title="Register"
        ContentTemplate="{DataTemplate local:Register}"
        Route="Register"/>

</Shell>
